{
  "name": "app-vue",
  "description": "Stable version of a basic SPA example with SCSS, Vue and TypeScript.",
  "license": "MIT",
  "version": "3.0.0",
  "scripts": {
    "build": "vue-cli-service build --dest \"../wwwroot\"",
    "test": "vue-cli-service test:unit",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "vue-cli-service lint",
    "lint:styles": "stylelint \"./src/**/*.(scss|vue)\"",
    "dev:client": "await tcp localhost:63414 && vue-cli-service build --watch --dest \"../wwwroot\"",
    "dev:dotnet": "await tcp localhost:5433 && dotnet run -p ..",
    "dev:infrastructure": "docker-compose -f \"../../../build/docker-compose-infrastructure.yml\" up",
    "start": "run-p -r dev:**"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@casl/ability": "^3.2.0",
    "@casl/vue": "^0.5.1",
    "@johmun/vue-tags-input": "^2.1.0",
    "bootstrap": "^4.3.1",
    "bootstrap-material-design": "^4.1.2",
    "bootstrap-vue": "^2.0.0-rc.19",
    "copy-to-clipboard": "^3.2.0",
    "core-js": "^2.6.5",
    "fetch-intercept": "^2.3.1",
    "inversify-props": "^1.3.2",
    "jquery": "^3.4.1",
    "localforage": "^1.7.3",
    "normalize-scss": "^7.0.1",
    "nprogress": "^0.2.0",
    "oidc-client": "^1.8.2",
    "popper.js": "^1.15.0",
    "qs": "^6.9.0",
    "reflect-metadata": "^0.1.13",
    "vee-validate": "^2.2.11",
    "vue": "^2.6.6",
    "vue-i18n": "^8.10.0",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^3.0.34",
    "vue-toasted": "^1.1.27",
    "vue2-timepicker": "^0.1.4",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/vue": "^3.0.0",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.5.0",
    "babel-core": "7.0.0-bridge.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^5.0.0",
    "require.all": "^2.0.4",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.5.4",
    "terser-webpack-plugin": "^1.3.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.5.21",
    "wait-cli": "^1.0.0"
  }
}
